@model IEnumerable<Sito.Models.Order>

@{
    ViewBag.Title = "Ordini";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Ordini</h2>

@Html.ValidationSummary(false, "", new { @class = "text-danger" })

@if (Model.Count() > 0)
{
    <div class="container">
        @foreach (Sito.Models.Order item in Model)
        {
            Html.RenderPartial("OrderSub", item);
        }
    </div>
}
